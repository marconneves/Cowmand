{
  "name": "cowmand",
  "version": "0.6.1",
  "description": "Fast helper to create a cli",
  "type": "module",
  "tags": [
    "cli",
    "command",
    "args",
    "argv",
    "helper",
    "command-line-interface"
  ],
  "main": "./dist/cjs/cowmand.js",
  "types": "./dist/cjs/cowmand.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/cowmand.js",
      "require": "./dist/cjs/cowmand.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MarconNeves/Cowmand"
  },
  "author": "Marcon Willian <oi@marcon.run>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "npm run build:esm && npm run build:cjs",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --watchAll",
    "test:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "lint": "eslint --ext .ts ./"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "debug": "^4.3.4",
    "jsonfile": "^6.1.0",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/jsonfile": "^6.1.4",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.2.9",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
